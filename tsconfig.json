{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "lib/*": ["src/lib/*"],
      "screens/*": ["src/screens/*"],
      "services/*": ["src/services/*"],
      "shared/*": ["src/shared/*"],
      "stores/*": ["src/stores/*"],
      "utils/*": ["src/utils/*"],
      "methods/*": ["src/methods/*"],
      "env": [".env"]
    },
    "skipLibCheck": true
  }
}
